{
  "ci": {
    "collect": {
      "numberOfRuns": 2,
      "startServerCommand": "cd frontend && npm start -- -p 3000",
      "url": ["http://localhost:3000/", "http://localhost:3000/about"]
    },
    "assert": {
      "assertions": {
        "categories:performance": [
          "warn",
          {
            "minScore": 0.6
          }
        ],
        "categories:accessibility": [
          "warn",
          {
            "minScore": 0.7
          }
        ],
        "categories:seo": [
          "warn",
          {
            "minScore": 0.7
          }
        ]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
